{
  "name": "deduction",
  "custom": {
    "webpack": {
      "configPath": "config/webpack.config.js"
    },
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": [
        "Content-Type",
        "X-Amz-Date",
        "Authorization",
        "X-Api-Key"
      ]
    }
  },
  "plugins": [
    "serverless-webpack-plugin",
    "serverless-cors-plugin"
  ]
}
